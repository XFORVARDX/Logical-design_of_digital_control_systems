set -tmpdir __projnav
set -xsthdpdir ./xst
run
-ifn seg_b.prj
-ifmt mixed
-ofn seg_b
-ofmt NGC
-p xc9500xl
-top seg_b
-opt_mode Speed
-opt_level 1
