set -tmpdir __projnav
set -xsthdpdir ./xst
run
-ifn seg_ee.prj
-ifmt mixed
-ofn seg_ee
-ofmt NGC
-p xc9500xl
-top seg_ee
-opt_mode Speed
-opt_level 1
